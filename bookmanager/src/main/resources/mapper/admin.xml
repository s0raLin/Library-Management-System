<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cangli.mapper.AdminMapper">
    <select id="findById" parameterType="Integer" resultType="com.cangli.pojo.Admin">
        select * from admin where id=#{id}
    </select>

    <select id="findAll" resultType="com.cangli.pojo.Admin">
        select * from admin
    </select>
    <insert id="add" parameterType="com.cangli.pojo.Admin">
        insert into admin values (null, #{username}, #{password},#{role})
    </insert>

</mapper>
